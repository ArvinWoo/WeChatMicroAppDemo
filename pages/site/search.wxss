/* pages/site/search.wxss */
page{
  background: #f2f2f2;
}
.swiper-tab{  
    width: 90%;  
    border-bottom: 1px solid #FFFFFF;  
    text-align: left;  
    line-height: 40px;
    margin: 0 auto;
}  
.swiper-tab-list{ 
  font-size: 14px;  
  display: inline-block;  
  width: 50%;  
  color: #999999;  
  text-align: center;
}  
.on{
  color: #237ECA;  
  border-bottom: 2px solid #237ECA;
}  
  
.swiper-box{
  display: block; 
  height: 100%; 
  width: 100%;
  overflow: hidden; 
}  
.swiper-box view{  
    text-align: center;  
}
.visHide{
  visibility: hidden;
}
.visShow{
  visibility: visible;
}
.lineSearchImg{
  width:20px;
  height:20px;
  position:relative;
  top:5px;
  left: 20px;
}
.lineSearchUnit{
  color:#237ECA;
  float:left;
}
.searchLineDIV{
  float:left;
  color:#999999;
  font-size:14px;
  width:100%;
  height:40px; 
}
.searchLinePlaceholder{
  float:left;
  height:40px;
  width:220px;
}
.disNone{
  display: none;
}
.t2_content{
  width: 97%;
  padding-bottom:2%; 
  background-color: white;
  margin-left: 1.2%;
  border-radius: 5px;
  overflow: hidden;
  margin-top:2%; 
}
.t2_content_f{
  margin-left: 3%;
  margin-top: 2%;
  float: left;
  width: 100%;
}
.t2_content_t{
  font-size: 16px;
  color: #3e7bc4;
  display: inline-block;
}
.t2_content_s{
  margin-left: 9px;
  font-size: 19px;
  color: #3e7bc4;
  display: inline-block;
}
.t2_content_1{
  font-size: 11px;
  color: red;
  display: inline-block;
  float: right;
  margin-right: 9%;
}
.t2_content_fg{
  margin-left: 3%;
  margin-top: 2%;
  float: left;
  width: 100%;
  font-size: 13px;
}
.t2_content_f2{
  color: #b1bac3;
  display: inline-block;
}
.t2_content_f3{
  margin-right: 9%;
  display: inline-block;
  float: right;
}
.t2_background{
  height:100%;
  width:100%;
  padding-top: 1%;
}
.t2_marBottomPer2{
  margin-bottom: 2%;
  height: 1px;
}
/* 浮动元素样式 */
.dingwei{
  height: 30px;
  width: 30px;
  position: absolute;
  left: 5%;
  top: 85%;
}
.tuoyuan{
  height: 80px;
  width: 80px;
  position: fixed;
  left: 75%;
  top: 85%;
}
.tuoyuanFont{
  position: absolute;
  top: 28%;
  left: 28%;
  color: #fff;
}